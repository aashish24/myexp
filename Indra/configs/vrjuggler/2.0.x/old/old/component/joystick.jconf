<?xml version="1.0" encoding="UTF-8"?>
<?org-vrjuggler-jccl-settings configuration.version="3.0"?>
<configuration xmlns="http://www.vrjuggler.org/jccl/xsd/3.0/configuration" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="Configuration" xsi:schemaLocation="http://www.vrjuggler.org/jccl/xsd/3.0/configuration http://www.vrjuggler.org/jccl/xsd/3.0/configuration.xsd">
   <elements>
      <input_manager name="JoyInput Manager" version="2">
         <driver_path>${VJ_BASE_DIR}/lib/gadgeteer/drivers</driver_path>
         <driver_path>${VJ_BASE_DIR}/lib32/gadgeteer/drivers</driver_path>
         <driver>LinuxJoydev_drv</driver>
      </input_manager>
      <linux_joydev name="Joystick" version="1">
         <port_name>/dev/js0</port_name>
         <min>0.0</min>
         <max>255.0</max>
         <device_host>viz0</device_host>
      </linux_joydev>
      <alias name="VJoyButton0" version="1">
         <proxy>VJoyButton0Proxy</proxy>
      </alias>
      <alias name="VJoyButton1" version="1">
         <proxy>VJoyButton1Proxy</proxy>
      </alias>
      <alias name="VJoyButton2" version="1">
