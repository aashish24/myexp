      </alias>
      <alias name="VJoyButton8" version="1">
         <proxy>VJoyButton8Proxy</proxy>
      </alias>
      <alias name="VJoyButton9" version="1">
         <proxy>VJoyButton9Proxy</proxy>
      </alias>
      <alias name="VJoyAxisYaw" version="1">
         <proxy>VJoyAxisYawProxy</proxy>
      </alias>
      <alias name="VJoyAxisPitch" version="1">
         <proxy>VJoyAxisPitchProxy</proxy>
      </alias>
      <digital_proxy name="VJoyButton0Proxy" version="1">
         <device>Joystick</device>
         <unit>0</unit>
      </digital_proxy>
      <digital_proxy name="VJoyButton1Proxy" version="1">
         <device>Joystick</device>
