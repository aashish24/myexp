      <machine_specific name="Node(ptah-1)" version="1">
         <display_system>
            <display_system name="display_system 0" version="1">
               <number_of_pipes>1</number_of_pipes>
               <x11_pipes>-1</x11_pipes>
            </display_system>
         </display_system>
         <display_windows>
            <display_window name="SimWindow1" version="2">
               <origin>0</origin>
               <origin>200</origin>
               <size>600</size>
               <size>600</size>
