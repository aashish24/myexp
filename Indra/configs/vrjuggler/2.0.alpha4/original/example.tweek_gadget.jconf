      <input_manager name="Tweek Input Manager" version="2">
         <driver_path>${VJ_BASE_DIR}/lib/gadgeteer/drivers</driver_path>
         <driver_path>${VJ_BASE_DIR}/lib32/gadgeteer/drivers</driver_path>
         <driver>TweekGadget_drv</driver>
      </input_manager>
      <tweek_gadget_gui name="TweekGadget GUI" version="1">
         <digital_device />
         <digital_device />
      </tweek_gadget_gui>
      <digital_proxy name="Button0Proxy" version="1">
         <device>My Tweek Device</device>
         <unit>0</unit>
      </digital_proxy>
      <digital_proxy name="Button2Proxy" version="1">
         <device>My Tweek Device</device>
         <unit>2</unit>
      </digital_proxy>
